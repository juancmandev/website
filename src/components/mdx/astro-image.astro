---
import { Image } from "astro:assets";

const props = Astro.props;
---

<Image
  id="img"
  src={props.src}
  alt={props.alt}
  width={props.width}
  height={props.height}
  class="w-auto h-auto rounded-md aspect-auto"
/>

<script>
  const image = document.getElementById("img")!;

  image && image.setAttribute("loading", "eager");
</script>
